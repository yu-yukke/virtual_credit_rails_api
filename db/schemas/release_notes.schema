index_key  = 'index_release_notes_on_'

create_table 'release_notes', id: :uuid, force: :cascade, charset: 'utf8mb4' do |t|
  t.string      'version',    null: false,
                              limit: 191

  t.string      'title',      null: false,
                              limit: 191

  t.timestamp   'created_at'
  t.timestamp   'updated_at'
end
